class Person {
    String n;

    public Person(String n) {
        this.n = n;
    }

    @Override
    public String toString() {
        return "Person{name:'" + n + "'}";
    }

    public static void main(String[] args) {
      
        Person p = new Person("Dinesh kumar");
      
        System.out.println(p.toString());
        }
}
