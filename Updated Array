class Main {
    public static void main(String[] args) {
        String[] array = {"Amsa", "Rakshu"};
        for (int i = 0; i < array.length; i++) {
            if(array[i] == "Amsa"){
                array[i]="Manoj";
                System.out.println("updated name:"+array[i]);
            }
            System.out.println(array[i]);
        }
    }
}
